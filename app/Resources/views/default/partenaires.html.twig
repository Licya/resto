{# app/Resources/views/default/partenaires.html.twig #}
{% extends '::trattoriaBase.html.twig' %}
{% block pageTitle %}<h1 id="title">Nos Partenaires</h1>{% endblock %}
{% block image %}partenaires{% endblock %}
{% block navigation %}{{ include ('default/siteNavigation.html.twig') }}{% endblock %}    
{% block content %}
    <table class="u-full-width" >
        {% for Partners in partners %}
            {%if Partners.enable == 1%}
                <tbody >
                    <tr>
                        <td class="partner-name">{{Partners.name}} </td>
                        <td class="tbl"><a href='{{Partners.websiteLink}}' class="tbl">{{Partners.websiteLink}}</a></td>
                    </tr>
                </tbody>

            {% endif %}
        {% endfor %}
    </table>
{% endblock %}